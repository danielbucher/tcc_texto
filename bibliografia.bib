	abstract = {This paper presents the experience of the Educandow Project in using the environment TecCiencia using of digital resources for the purpose of teaching. TecCiencia is a social network that brings together various media resources, enabling the creation of educational communities, using the free platform Noosfero. It presents the first results obtained by using this environment in a pedagogical-didactic contemporary perspective of a social network for education by students from the SESI \School in the city of Candeias/BA.},
	author = {D. A. Santos, A. F. Schwarzelmüller, A. Lima},
	title = {Projeto Educandow: experimentando uso de rede social como apoio ao ensino fundamental},
	year = {2012},
}

